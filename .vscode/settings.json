{
  "prettier.documentSelectors": ["**/*.astro, **/*.js, **/*.ts, **/*.vue"],
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.validate": [
    "javascript", // Enable .js
    "astro", // Enable .astro
    "vue", // Enable .vue
    "typescript" // Enable .ts
  ]
}
